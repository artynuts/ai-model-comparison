generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model QueryHistory {
  id        String   @id @default(cuid())
  query     String
  timestamp BigInt
  responses Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([timestamp])
}
